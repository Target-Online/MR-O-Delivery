    "android": {
      "package": "com.mro.mrodelivery",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBaHGSfahA2b9YS3Pvguelo2yN8heE41cw"
        }
      }
    },
    "packagerOpts": {
      "assetExts": [
        "ttf"
